<h3>Search Authors</h3>
<form action="/authors" method="GET">
    <lable>Name</lable>
    <input type="text" name="firstname" value="<%= searchQuery.firstname %>">
    <button type="submit">Search</button>
</form>
<br>
<% authors.forEach(author => { %>
    <div>
        <%= author.firstname + " " + author.surname  %>
    </div>
<% }) %>